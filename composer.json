        "multidimensional/xml-array": "~1",
        "multidimensional/array-validation": "~1",
        "multidimensional/array-sanitization": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
