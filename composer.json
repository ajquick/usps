    "autoload": {
        "psr-4": {
            "Multidimensional\\USPS\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Multidimensional\\USPS\\Test\\": "tests"
        }
    },
    "repositories": {
        "multidimensional/dom-array": {
            "type": "vcs",
            "url": "https://ajquick:DDnS9UV75DBSFNE8w79a@github.com/ajquick/dom-array.git"
        },
        "multidimensional/xml-array": {
            "type": "vcs",
            "url": "https://ajquick:DDnS9UV75DBSFNE8w79a@github.com/ajquick/xml-array.git"
        },
        "multidimensional/array-validation": {
            "type": "vcs",
            "url": "https://ajquick:DDnS9UV75DBSFNE8w79a@github.com/ajquick/array-validation.git"
        },
        "multidimensional/array-sanitization": {
            "type": "vcs",
            "url": "https://ajquick:DDnS9UV75DBSFNE8w79a@github.com/ajquick/array-sanitization.git"
