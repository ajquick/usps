{
    "name": "multidimensional/usps",
    "description": "A USPS API Library for PHP.",
    "authors": [
        {
            "name": "multidimension.al",
            "homepage": "https://multidimension.al",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~6.0",
        "multidimensional/dom-array": "dev-master",
        "multidimensional/xml-array": "dev-master",
        "multidimensional/array-validation": "dev-master",
        "multidimensional/array-sanitization": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
